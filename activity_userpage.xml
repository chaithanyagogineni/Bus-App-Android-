<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="user1.myproject.com.test1.userpage"
    android:id="@+id/userpage"
    android:background="#ff63ff17">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/in1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:src="@drawable/inimage" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/out1"
        android:src="@drawable/outimage"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="IN-REPORT"
        android:id="@+id/t1"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/in1"
        android:layout_alignEnd="@+id/out1"
        android:layout_marginRight="125dp"
        android:layout_marginEnd="125dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="OUT-REPORT"
        android:id="@+id/t2"
        android:layout_centerVertical="true"
        android:layout_alignRight="@+id/t1"
        android:layout_alignEnd="@+id/t1" />

</RelativeLayout>
